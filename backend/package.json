    "react-dom": "^18.3.1",
    "react-email": "^3.0.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "vite": "^5.4.11"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@10.0.0",
  "pnpm": {
    "overrides": {
      "next": "15.1.11",
      "@mikro-orm/core": "6.4.16",
