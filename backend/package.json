    "@rokmohar/medusa-plugin-meilisearch": "^1.3.5",
    "awilix": "^8.0.1",
    "medusajs-launch-utils": "^0.0.18",
    "minio": "^8.0.3",
    "next": "^15.5.9",
    "pg": "^8.13.1",
    "resend": "^4.0.1",
    "shippo": "^2.18.0",
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@medusajs/test-utils": "2.12.5",
    "@mikro-orm/cli": "6.4.3",
    "@swc/core": "1.5.7",
    "@swc/jest": "^0.2.37",
